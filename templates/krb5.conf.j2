[logging]
 default = FILE:/var/log/krb5libs.log
 kdc = FILE:/var/log/krb5kdc.log
 admin_server = FILE:/var/log/kadmind.log

[libdefaults]
 dns_lookup_realm = false
 dns_lookup_kdc = false
 ticket_lifetime = 24h
 renew_lifetime = 7d
 forwardable = true
 rdns = false
 default_realm = ENV1.LAB.NZ
 #default_ccache_name = KEYRING:persistent:%{uid}

[realms]
        ENV1.LAB.NZ   = {
                kdc = AD.ENV1.LAB.NZ
                default_domain = ENV1
        }
[domain_realm]
        ENV1 = ENV1.LAB.NZ  
